          <xs:documentation>Data value of the attribute</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="type" type="ExtendedAttributeType">
        <xs:annotation>
          <xs:documentation>Data type of the attribute.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
