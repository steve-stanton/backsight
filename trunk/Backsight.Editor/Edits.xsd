<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="Edits" targetNamespace="http://tempuri.org/Edits.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/Edits.xsd" xmlns:mstns="http://tempuri.org/Edits.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="OperationType" abstract="true" id="Id">
    <xs:attribute name="Id" type="xs:string" use="required" />
  </xs:complexType>
  <xs:complexType name="AttachPointType">
    <xs:complexContent>
      <xs:extension base="OperationType">
        <xs:sequence>
          <xs:element name="Point" type="FeatureDataType" />
        </xs:sequence>
        <xs:attribute name="Line" type="xs:string" use="required" />
        <xs:attribute name="PositionRatio" type="xs:unsignedInt" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FeatureDataType">
    <xs:attributeGroup ref="FeatureDataGroup" />
  </xs:complexType>
  <xs:complexType name="FeatureType" abstract="true" id="Id">
    <xs:attribute name="Id" type="xs:string" />
    <xs:attribute name="Type" type="xs:int" />
    <xs:attribute name="Key" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="PointFeatureType">
    <xs:complexContent>
      <xs:extension base="FeatureType">
        <xs:sequence>
        </xs:sequence>
        <xs:attribute name="X" type="xs:unsignedLong" use="required" />
        <xs:attribute name="Y" type="xs:unsignedLong" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SharedPointType">
    <xs:complexContent>
      <xs:extension base="FeatureType">
        <xs:sequence />
        <xs:attribute name="FirstPoint" type="xs:string" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="IdType">
    <xs:attribute name="Session" type="xs:unsignedInt" use="required" />
    <xs:attribute name="Order" type="xs:unsignedInt" use="required" />
  </xs:complexType>
  <xs:attributeGroup name="FeatureDataGroup">
    <xs:attribute name="Id" type="xs:string" />
    <xs:attribute name="Type" type="xs:int" />
    <xs:attribute name="Key" type="xs:string" use="optional" />
  </xs:attributeGroup>
  <xs:complexType name="CircleType">
    <xs:attribute name="Center" type="xs:string" />
    <xs:attribute name="Radius" type="xs:double" />
  </xs:complexType>
  <xs:complexType name="LineFeatureType">
    <xs:complexContent>
      <xs:extension base="FeatureType">
        <xs:sequence>
        </xs:sequence>
        <xs:attribute name="From" type="xs:string" />
        <xs:attribute name="To" type="xs:string" />
        <xs:attribute name="IsTopological" type="xs:boolean" use="optional" default="true" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MultiSegmentType">
    <xs:complexContent>
      <xs:extension base="LineFeatureType">
        <xs:sequence>
          <xs:element name="Point" type="PointGeometryType" minOccurs="2" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PointGeometryType">
    <xs:sequence />
    <xs:attribute name="X" type="xs:unsignedLong" use="required" />
    <xs:attribute name="Y" type="xs:unsignedLong" use="required" />
  </xs:complexType>
  <xs:complexType name="SectionType">
    <xs:complexContent>
      <xs:extension base="LineFeatureType">
        <xs:attribute name="Base" type="xs:string" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ArcType">
    <xs:complexContent>
      <xs:extension base="LineFeatureType">
        <xs:sequence>
          <xs:choice maxOccurs="1" minOccurs="0">
            <xs:element name="FirstArc" type="xs:string" />
            <xs:element name="Circle" type="CircleType" />
          </xs:choice>
        </xs:sequence>
        <xs:attribute name="IsClockwise" type="xs:boolean" use="optional" default="true" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="IdType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]+.[0-9]+" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>